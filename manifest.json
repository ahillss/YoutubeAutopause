{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Youtube Autopause",
  "description": "Loads youtube videos paused",
  "host_permissions": [ "*://*.youtube.com/*" ],
  "content_scripts": [{
    "matches": [ "*://*.youtube.com/*" ],
    "run_at":"document_start",
    "js": [ "inject.js" ]
  }],
  "web_accessible_resources": [{
    "resources": [ "myscript.js" ],
    "matches": [ "<all_urls>" ]
  }]
}